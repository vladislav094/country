type Query {
    country(id: ID!): Country!
    countries(page: Int, size: Int): CountryConnection
}

type Mutation {
    addCountry(input: CountryInput!): Country!
}

type Country {
    id: ID!
    countryName: String!
    countryCode: String!
}

input CountryInput {
    countryName: String
    countryCode: String
}
